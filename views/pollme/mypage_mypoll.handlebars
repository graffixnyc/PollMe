<center>
    
<div class="user-home-mypolls">
    <h3 id="mypoll-heading" > My Polls </h3>
    <table class="poll-table">
        <thead>
            <tr>
                <th class="table-head">Question</th>
                <th class="table-head">Category</th>
                <th class="table-head">Date</th>
                <th class="table-head">Votes</th>
            </tr>
        </thead>
        <tbody>
            {{#each poll}}
            <tr>
                <td class="table-body"><a class="track-single" href="/poll/{{this._id}}">{{this.question}}</a></td>
                <td class="table-body">{{this.category}}</td>
                <td class="table-body">{{this.postedDate}}</td>
                <td class="table-body">{{this.votes}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>
</center>
